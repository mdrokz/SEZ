{
  "name": "SEZ",
  "version": "1.2.2",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run electron:start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "angular": "ng serve --port 4400",
    "postinstall": "electron-builder install-app-deps",
    "electron:start": "npm-run-all -p postinstall ng:clean ng:build electron:serve",
    "ng:clean": "rimraf dist && rimraf main.js",
    "electron:serve": "wait-on ./dist/electron-angular/index.html && npm run electron:watch2",
    "ng:build": "ng build --watch=true --poll 1000 ",
    "electron:tsc": "tsc -w main.ts --lib es2015,es2016,es2017,esnext,dom",
    "electron:run": "wait-on main.js && electron . --serve --console --development",
    "electron:exit": "taskkill /IM electron.exe /F",
    "electron:watch": "concurrently --kill-others \"npm run electron:run\"",
    "electron:watch2": "concurrently --kill-others \"npm run electron:tsc \"  \"npm run electron:run\"",
    "electron-pack": "electron-builder --dir",
    "electron-build": "electron-builder"
  },
  "build": {
    "productName": "SEZ",
    "appId": "com.shabab.sez",
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "files": [
      "*.js",
      "./main.js",
      "dist",
      "data-access",
      "models",
      "MongoDB"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mdrokz/SEZ.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.9",
    "@angular/cdk": "^8.0.0",
    "@angular/core": "~8.2.9",
    "@angular/material": "^7.1.1",
    "@nebular/auth": "^4.5.0",
    "@nebular/eva-icons": "4.5.0",
    "@nebular/security": "^4.5.0",
    "@nebular/theme": "^4.5.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "eva-icons": "^1.1.1",
    "jquery": "^3.4.1",
    "mongoose": "^5.7.12",
    "mongoose-unique-validator": "^2.0.3",
    "nebular-icons": "^1.1.0",
    "ng2-completer": "^3.0.3",
    "ng2-smart-table": "^1.5.0",
    "ngx-electron": "^2.2.0",
    "popper.js": "^1.16.0",
    "rxjs": "^6.0.0",
    "tslib": "^1.9.0",
    "yargs": "^12.0.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.8",
    "@angular/cli": "~8.3.8",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/forms": "~8.2.9",
    "@angular/http": "^7.1.3",
    "@angular/language-service": "~8.2.9",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/router": "~8.2.9",
    "pdfmake": "^0.1.62",
    "@schematics/angular": "~8.3.8",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.1",
    "electron": "^3.1.3",
    "electron-builder": "^20.34.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3",
    "wait-on": "^2.1.0"
  }
}
